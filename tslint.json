{
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-empty": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-duplicate-switch-case": true,
    "no-console": {
      "severity": "error",
      "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
    }
  }
}
